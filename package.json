  "babel": {
    "presets": [
      [
        "react-app",
        {
          "absoluteRuntime": false
        }
      ]
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build1": "cross-env BABEL_ENV=production babel src -d dist",
    "build": "react-scripts build -p jsconfig.json && yarn copy-files",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "release2": "auto shipit --base-branch=main",
    "release": "standard-version && npm publish",
    "copy-files": "copyfiles -u 1 \"src/**/*.css\" \"src/**/*.svg\" \"src/**/*.json\" \"-e src/stories/**/*.stories.jsx\" \"src/stories/**/*.jsx\" \"src/styles/**\" lib/",
    "prebuild": "rimraf lib",
    "export-lib": "copyfiles -u 1 \"-e src/stories/**/*.stories.jsx\" \"src/stories/**/*.jsx\" \"src/styles/**\" dist/"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@storybook/addon-actions": "^6.5.0-beta.8",
