    "@storybook/node-logger": "^6.5.0-beta.8",
    "@storybook/preset-create-react-app": "^4.1.0",
    "@storybook/react": "^6.5.0-beta.8",
    "@storybook/testing-library": "^0.0.11",
    "auto": "^10.37.6",
    "babel-plugin-named-exports-order": "^0.0.2",
    "chromatic": "^6.5.4",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
